{
  "cSpell.words": [
    "nextlint",
    "stylelint",
    "Vercel"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.alwaysShowStatus": true,
  "[css]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "css.validate": false,
  "scss.validate": false
}
